# TrajectryPlannerTester CMake config file
#
# This file sets the following variables:
# TrajectryPlannerTester_FOUND - Always TRUE.
# TrajectryPlannerTester_INCLUDE_DIRS - Directories containing the TrajectryPlannerTester include files.
# TrajectryPlannerTester_IDL_DIRS - Directories containing the TrajectryPlannerTester IDL files.
# TrajectryPlannerTester_LIBRARIES - Libraries needed to use TrajectryPlannerTester.
# TrajectryPlannerTester_DEFINITIONS - Compiler flags for TrajectryPlannerTester.
# TrajectryPlannerTester_VERSION - The version of TrajectryPlannerTester found.
# TrajectryPlannerTester_VERSION_MAJOR - The major version of TrajectryPlannerTester found.
# TrajectryPlannerTester_VERSION_MINOR - The minor version of TrajectryPlannerTester found.
# TrajectryPlannerTester_VERSION_REVISION - The revision version of TrajectryPlannerTester found.
# TrajectryPlannerTester_VERSION_CANDIDATE - The candidate version of TrajectryPlannerTester found.

message(STATUS "Found TrajectryPlannerTester-@PROJECT_VERSION@")
set(TrajectryPlannerTester_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(TrajectryPlannerTester_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(TrajectryPlannerTester_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(TrajectryPlannerTester_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(TrajectryPlannerTester_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(TrajectryPlannerTester_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(TrajectryPlannerTester_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(TrajectryPlannerTester_DEFINITIONS ${<dependency>_DEFINITIONS})

set(TrajectryPlannerTester_VERSION @PROJECT_VERSION@)
set(TrajectryPlannerTester_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(TrajectryPlannerTester_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(TrajectryPlannerTester_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(TrajectryPlannerTester_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

